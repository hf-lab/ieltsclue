function reset_cal_2(){document.getElementById("Listening_2").value="";document.getElementById("Listening_band").value="";document.getElementById("Reading_2").value="";document.getElementById("Reading_band").value="";document.getElementById("academic").checked=false;document.getElementById("training").checked=false;}function calculate_2(){var Listening_2=parseInt(document.getElementById("Listening_2").value);var Reading_2=parseInt(document.getElementById("Reading_2").value);if(isNaN(Listening_2)){document.getElementById("Listening_2").style="border:1px solid red;";}else{document.getElementById("Listening_2").style="border:1px solid lightgrey;";}if(isNaN(Reading_2)){document.getElementById("Reading_2").style="border:1px solid red;";}else{document.getElementById("Reading_2").style="border:1px solid lightgrey;";}var Listening_band="";var Reading_band="";if(Listening_2==0){Listening_band=0;}if(Listening_2>=1&&Listening_2<=2){Listening_band=1;}if(Listening_2==3){Listening_band=2;}if(Listening_2>=4&&Listening_2<=5){Listening_band=2.5;}if(Listening_2>=6&&Listening_2<=7){Listening_band=3;}if(Listening_2>=8&&Listening_2<=10){Listening_band=3.5;}if(Listening_2>=11&&Listening_2<=12){Listening_band=4;}if(Listening_2>=13&&Listening_2<=15){Listening_band=4.5;}if(Listening_2>=16&&Listening_2<=17){Listening_band=5;}if(Listening_2>=18&&Listening_2<=22){Listening_band=5.5;}if(Listening_2>=23&&Listening_2<=25){Listening_band=6;}if(Listening_2>=26&&Listening_2<=29){Listening_band=6.5;}if(Listening_2>=30&&Listening_2<=31){Listening_band=7;}if(Listening_2>=32&&Listening_2<=34){Listening_band=7.5;}if(Listening_2>=35&&Listening_2<=36){Listening_band=8;}if(Listening_2>=37&&Listening_2<=38){Listening_band=8.5;}if(Listening_2>=39&&Listening_2<=40){Listening_band=9;}document.getElementById("Listening_band").value=Listening_band;var academic=document.getElementById("academic").checked;var training=document.getElementById("training").checked;if(academic==false&&training==false){document.getElementById("radio1").style="color:red";document.getElementById("radio2").style="color:red";}else{document.getElementById("radio1").style="color:#333";document.getElementById("radio2").style="color:#333";if(academic==true){if(Reading_2==0){Reading_band=0;}if(Reading_2>=1&&Reading_2<=2){Reading_band=1;}if(Reading_2==3){Reading_band=2;}if(Reading_2>=4&&Reading_2<=5){Reading_band=2.5;}if(Reading_2>=6&&Reading_2<=7){Reading_band=3;}if(Reading_2>=8&&Reading_2<=9){Reading_band=3.5;}if(Reading_2>=10&&Reading_2<=12){Reading_band=4;}if(Reading_2>=13&&Reading_2<=14){Reading_band=4.5;}if(Reading_2>=15&&Reading_2<=18){Reading_band=5;}if(Reading_2>=19&&Reading_2<=22){Reading_band=5.5;}if(Reading_2>=23&&Reading_2<=26){Reading_band=6;}if(Reading_2>=27&&Reading_2<=29){Reading_band=6.5;}if(Reading_2>=30&&Reading_2<=32){Reading_band=7;}if(Reading_2>=33&&Reading_2<=34){Reading_band=7.5;}if(Reading_2>=35&&Reading_2<=36){Reading_band=8;}if(Reading_2>=37&&Reading_2<=38){Reading_band=8.5;}if(Reading_2>=39&&Reading_2<=40){Reading_band=9;}document.getElementById("Reading_band").value=Reading_band;}else if(training==true){if(Reading_2==0){Reading_band=0;}if(Reading_2==1){Reading_band=1;}if(Reading_2>=2&&Reading_2<=4){Reading_band=2;}if(Reading_2==5){Reading_band=2.5;}if(Reading_2>=6&&Reading_2<=8){Reading_band=2.5;}if(Reading_2>=9&&Reading_2<=11){Reading_band=3;}if(Reading_2>=12&&Reading_2<=14){Reading_band=3.5;}if(Reading_2>=15&&Reading_2<=18){Reading_band=4;}if(Reading_2>=19&&Reading_2<=22){Reading_band=4.5;}if(Reading_2>=23&&Reading_2<=26){Reading_band=5;}if(Reading_2>=27&&Reading_2<=29){Reading_band=5.5;}if(Reading_2>=30&&Reading_2<=31){Reading_band=6;}if(Reading_2>=32&&Reading_2<=33){Reading_band=6.5;}if(Reading_2>=34&&Reading_2<=35){Reading_band=7;}if(Reading_2==36){Reading_band=7.5;}if(Reading_2>=37&&Reading_2<=38){Reading_band=8;}if(Reading_2==39){Reading_band=8.5;}if(Reading_2==40){Reading_band=9;}document.getElementById("Reading_band").value=Reading_band;}}}
